[
  {
    "port": 8080,
    "routes": [
      {
        "path": "/users",
        "method": "GET",
        "response": {
          "status": 200,
          "body": [
            {
              "id_cliente": 1,
              "name": "User One",
              "email": "user1@example.com"
            },
            {
              "id_cliente": 2,
              "name": "User Two",
              "email": "user2@example.com"
            }
          ]
        }
      },
      {
        "path": "/users",
        "method": "GET",
        "query_params": [
          {
            "name": "id",
            "maps_to": "id_cliente"
          }
        ],
        "data": [
          {
            "id_cliente": 1,
            "name": "User One",
            "email": "user1@example.com"
          },
          {
            "id_cliente": 2,
            "name": "User Two",
            "email": "user2@example.com"
          }
        ],
        "response_on_match": {
          "status": 200
        },
        "response_on_no_match": {
          "status": 404,
          "body": {
            "error": "User not found"
          }
        }
      },
      {
        "path": "/users/{id}",
        "method": "GET",
        "path_params": [
          {
            "name": "id",
            "maps_to": "id_cliente"
          }
        ],
        "data": [
          {
            "id_cliente": 1,
            "name": "User One",
            "email": "user1@example.com"
          },
          {
            "id_cliente": 2,
            "name": "User Two",
            "email": "user2@example.com"
          }
        ],
        "response_on_match": {
          "status": 200
        },
        "response_on_no_match": {
          "status": 404,
          "body": {
            "error": "User not found"
          }
        }
      }
    ]
  },
  {
    "port": 8081,
